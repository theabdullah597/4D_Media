    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start"
  },
  "keywords": [
    "printing",
    "custom-merchandise",
    "e-commerce"
  ],
  "author": "4D Media",
  "license": "ISC",
  "dependencies": {
    "recharts": "^3.7.0",
    "sqlite3": "^5.1.7"
  }
}
